<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE eps2probandltf_config>

<netcdfloadconfiguration xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="netcdfLoad.configuration.xsd">

	<load>
		<netcdf variable_name="air_temperature_2m" />
		<wdb name="air temperature" units="K"/>
	</load>

	<load>
		<netcdf variable_name="precipitation_amount"/>
		<wdb name="lwe thickness of precipitation amount" units="kg/m2" validfrom="validtime - 6 hours"/>
	</load>

	<load>
		<netcdf variable_name="cloud_area_fraction">
			<dimension name="percentile" value="0.1" />
		</netcdf>
		<wdb name="10th percentile of cloud area fraction" units="K" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction">
			<dimension name="percentile" value="0.25" />
		</netcdf>
		<wdb name="25th percentile of cloud area fraction" units="K" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction">
			<dimension name="percentile" value="0.5" />
		</netcdf>
		<wdb name="50th percentile of cloud area fraction" units="K" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction">
			<dimension name="percentile" value="0.75" />
		</netcdf>
		<wdb name="75th percentile of cloud area fraction" units="K" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction">
			<dimension name="percentile" value="0.9" />
		</netcdf>
		<wdb name="90th percentile of cloud area fraction" units="K" />
	</load>
	

</netcdfloadconfiguration>