<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE eps2probandltf_config>

<netcdfloadconfiguration xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="netcdfLoad.configuration.xsd">

	<load>
		<netcdf cfname="altitude" />
		<wdb name="altitude" validfrom="-infinity" validto="infinity"/>
	</load>

	<load>
		<netcdf cfname="precipitation_amount_low_estimate"/>
		<wdb name="low estimate of precipitation amount" validfrom="validtime - 1 hours"/>
	</load>
	<load>
		<netcdf cfname="precipitation_amount_middle_estimate"/>
		<wdb name="middle estimate of precipitation amount" validfrom="validtime - 1 hours"/>
	</load>
	<load>
		<netcdf cfname="precipitation_amount_high_estimate"/>
		<wdb name="high estimate of precipitation amount" validfrom="validtime - 1 hours"/>
	</load>
	
	<load>
		<netcdf cfname="precipitation_amount_prob_low"/>
		<wdb name="probability of precipitation" validfrom="validtime - 1 hours"/>
	</load>

	<load>
		<netcdf cfname="thunderstorm_index_combined"/>
		<wdb name="thunderstorm probability"/>
	</load>

	<load>
		<netcdf cfname="precipitation_amount_acc"/>
		<wdb name="precipitation amount" validfrom="referencetime"/>
	</load>

	<load>
		<netcdf cfname="land_area_fraction"/>
		<wdb name="land area fraction" validfrom="-infinity" validto="infinity"/>
	</load>

	<load>
		<netcdf cfname="cloud_area_fraction"/>
		<wdb name="cloud area fraction"/>
	</load>
	<load>
		<netcdf cfname="air_temperature_2m"/>
		<wdb name="air temperature">
			<level name="height above ground" value="2" />
		</wdb>
	</load>

	<load>
		<netcdf cfname="relative_humidity_2m"/>
		<wdb name="relative humidity">
			<level name="height above ground" value="2" />
		</wdb>
	</load>

	<load>
		<netcdf cfname="x_wind_10m"/>
		<wdb name="x wind">
			<level name="height above ground" value="10" />
		</wdb>
	</load>
	<load>
		<netcdf cfname="y_wind_10m"/>
		<wdb name="y wind">
			<level name="height above ground" value="10" />
		</wdb>
	</load>

	<load>
		<netcdf cfname="fog_area_fraction"/>
		<wdb name="cloud area fraction in atmosphere layer">
			<level name="atmosphere sigma coordinate" value="1000"/>
		</wdb>
	</load>
 
	<load>
		<netcdf cfname="high_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer">
			<level name="atmosphere sigma coordinate" value="300"/>
		</wdb>
	</load>
	<load>
		<netcdf cfname="medium_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer">
			<level name="atmosphere sigma coordinate" value="500"/>
		</wdb>
	</load>
	<load>
		<netcdf cfname="low_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer">
			<level name="atmosphere sigma coordinate" value="850"/>
		</wdb>
	</load>

	<load>
		<netcdf cfname="air_pressure_at_sea_level"/>
		<wdb name="air pressure at sea level"/>
	</load>


</netcdfloadconfiguration>
