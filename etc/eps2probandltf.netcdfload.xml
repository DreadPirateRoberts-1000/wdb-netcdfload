<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE eps2probandltf_config>

<netcdfloadconfiguration xmlns:xi="http://www.w3.org/2001/XInclude"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="netcdfLoad.configuration.xsd">

	<load>
		<netcdf variable_name="altitude" />
		<wdb name="altitude" units="m" validfrom="-infinity" validto="infinity"/>
	</load>

	<load>
		<netcdf variable_name="weather_symbol" />
		<wdb name="yr weather symbol" units="1" validfrom="validTime - 6 hours" />
	</load>
	<load>
		<netcdf variable_name="weather_symbol_confidence" />
		<wdb name="probability of yr weather symbol" units="%" validfrom="validTime - 6 hours" />
	</load>

	<load>
		<netcdf variable_name="cloud_area_fraction" />
		<wdb name="cloud area fraction" units="%"/>
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction_percentiles">
			<dimension name="percentile" value="0.1" />
		</netcdf>
		<wdb name="10th percentile of cloud area fraction" units="%" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction_percentiles">
			<dimension name="percentile" value="0.25" />
		</netcdf>
		<wdb name="25th percentile of cloud area fraction" units="%" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction_percentiles">
			<dimension name="percentile" value="0.5" />
		</netcdf>
		<wdb name="50th percentile of cloud area fraction" units="%" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction_percentiles">
			<dimension name="percentile" value="0.75" />
		</netcdf>
		<wdb name="75th percentile of cloud area fraction" units="%" />
	</load>
	<load>
		<netcdf variable_name="cloud_area_fraction_percentiles">
			<dimension name="percentile" value="0.9" />
		</netcdf>
		<wdb name="90th percentile of cloud area fraction" units="%" />
	</load>
	<load>
		<netcdf variable_name="low_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer" units="%">
			<level name="atmosphere sigma coordinate" value="850"/>
		</wdb>
	</load>
	<load>
		<netcdf variable_name="medium_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer" units="%">
			<level name="atmosphere sigma coordinate" value="500"/>
		</wdb>
	</load>
	<load>
		<netcdf variable_name="high_type_cloud_area_fraction" />
		<wdb name="cloud area fraction in atmosphere layer" units="%">
			<level name="atmosphere sigma coordinate" value="300"/>
		</wdb>
	</load>
	
	<load>
		<netcdf variable_name="air_temperature_2m" />
		<wdb name="air temperature" units="K"/>
	</load>
	<load>
		<netcdf variable_name="air_temperature_2m_min6h" />
		<wdb name="min air temperature" units="K" validfrom="validTime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="air_temperature_2m_max6h" />
		<wdb name="max air temperature" units="K" validfrom="validTime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="air_temperature_percentiles">
			<dimension name="percentile" value="0.1" />
		</netcdf>
		<wdb name="10th percentile of air temperature" units="K"/>
	</load>
	<load>
		<netcdf variable_name="air_temperature_percentiles">
			<dimension name="percentile" value="0.25" />
		</netcdf>
		<wdb name="25th percentile of air temperature" units="K" />
	</load>
	<load>
		<netcdf variable_name="air_temperature_percentiles">
			<dimension name="percentile" value="0.5" />
		</netcdf>
		<wdb name="50th percentile of air temperature" units="K" />
	</load>
	<load>
		<netcdf variable_name="air_temperature_percentiles">
			<dimension name="percentile" value="0.75" />
		</netcdf>
		<wdb name="75th percentile of air temperature" units="K" />
	</load>
	<load>
		<netcdf variable_name="air_temperature_percentiles">
			<dimension name="percentile" value="0.9" />
		</netcdf>
		<wdb name="90th percentile of air temperature" units="K" />
	</load>
	<load>
		<netcdf variable_name="air_temperature_confidence">
			<dimension name="interval_of_air_temperature" value="1.5"/>
		</netcdf>
		<wdb name="probability of air temperature +/-1.5" units="%" />
	</load>
	<load>
		<netcdf variable_name="air_temperature_confidence">
			<dimension name="interval_of_air_temperature" value="2.5"/>
		</netcdf>
		<wdb name="probability of air temperature +/-2.5" units="%" />
	</load>

	<load>
		<netcdf variable_name="x_wind_10m"/>
		<wdb name="x wind" units="m/s"/>
	</load>
	<load>
		<netcdf variable_name="y_wind_10m"/>
		<wdb name="y wind" units="m/s"/>
	</load>
	<load>
		<netcdf variable_name="wind_speed_confidence"/>
		<wdb name="probability of wind speed" units="%"/>
	</load>

	<load>
		<netcdf variable_name="precipitation_amount"/>
		<wdb name="lwe thickness of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_percentiles">
			<dimension name="percentile" value="0.1"/>
		</netcdf>
		<wdb name="10th percentile of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_percentiles">
			<dimension name="percentile" value="0.25"/>
		</netcdf>
		<wdb name="25th percentile of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_percentiles">
			<dimension name="percentile" value="0.5"/>
		</netcdf>
		<wdb name="50th percentile of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_percentiles">
			<dimension name="percentile" value="0.75"/>
		</netcdf>
		<wdb name="75th percentile of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_percentiles">
			<dimension name="percentile" value="0.9"/>
		</netcdf>
		<wdb name="90th percentile of precipitation amount" units="m" alternativeunitconversion="1000 kg/m2" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="0.1"/>
		</netcdf>
		<wdb name="probability of precipitation >0.1mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="0.2"/>
		</netcdf>
		<wdb name="probability of precipitation >0.2mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="0.5"/>
		</netcdf>
		<wdb name="probability of precipitation >0.5mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="1"/>
		</netcdf>
		<wdb name="probability of precipitation >1.0mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="2"/>
		</netcdf>
		<wdb name="probability of precipitation >2.0mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="precipitation_limit_confidence">
			<dimension name="precipitation_limit" value="5"/>
		</netcdf>
		<wdb name="probability of precipitation >5.0mm" units="%" validfrom="validtime - 6 hours"/>
	</load>
	<load>
		<netcdf variable_name="relative_humidity_2m"/>
		<wdb name="relative humidity" units="%"/>
	</load>
	<load>
		<netcdf variable_name="sea_level_pressure"/>
		<wdb name="air pressure at sea level" units="Pa"/>
	</load>

</netcdfloadconfiguration>
